<h1>Vista Index de Pages</h1>
<p>The current time is <%=Time.now%></p>
<table>
<thead>
  <tr>
    <th>Title</th>
    <th>Body</th>
    <th colspan="3"></th>
  </tr>
</thead>

<tbody>
  <% @pages.each do |page| %>
    <tr>
      <td><%= page.title %></td>
      <td><%= page.body %></td>
      <td><%= page.slug %></td>
      
    </tr>
  <% end %>
</tbody>
</table>